        "symfony/polyfill-apcu": "^1.0",
        "symfony/polyfill-php73": "^1.10",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/symfony": "^3.4",
        "symfony/twig-bridge": "^3.4",
