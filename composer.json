        "prestashop/ps_categorytree": "^2",
        "prestashop/ps_checkpayment": "^2",
        "prestashop/ps_contactinfo": "^3",
        "prestashop/ps_currencyselector": "^2",
        "prestashop/ps_customeraccountlinks": "^3",
