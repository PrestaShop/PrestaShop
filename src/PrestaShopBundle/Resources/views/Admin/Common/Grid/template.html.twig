<div class="card">
  <div class="card-header js-grid-header">
    <h3 class="d-inline-block card-header-title">
      {{ grid.name }} ()
    </h3>
  </div>

  <div class="card-body">
    <div class="row">
      <div class="col">
        <table>
          <tbody>
            {% for row in grid.table.data %}
              <tr>
                {% for type, column_data in row %}
                  <td class="{{ type }}-type">
                    {{ dump(column_data) }}
                  </td>
                {% endfor %}
              </tr>
            {% endfor %}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
