{% extends 'PrestaShopBundle:Admin:layout.html.twig' %}

{% block stylesheets %}

    {{ parent() }}

    {% stylesheets '@PrestaShopBundle/Resources/public/admin/css/module/module.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% stylesheets '@PrestaShopBundle/Resources/public/admin/css/plugins/jquery.prestagify.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

{% endblock %}

{% block javascripts %}

    {{ parent() }}

    {% javascripts '@PrestaShopBundle/Resources/public/admin/js/module/module.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts '@PrestaShopBundle/Resources/public/admin/js/plugins/jquery.prestagify.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% set requireAddonSearch = false %}

{% block content %}
    {{ moduleContent|raw }}
{% endblock %}
