{{ include('@AdminThemes/new-theme/public/preload.html.twig') }}

{# Javascript files from legacy Controller::css_files #}
{% for cssUri, cssMedia in css_files %}
  <link href="{{ cssUri|e('html') }}" rel="stylesheet" type="text/css"/>
{% endfor %}

{# Javascript files from legacy Controller::js_files #}
{% if js_files is not empty %}
  {% for jsUrl in js_files %}
    <script type="text/javascript" src="{{ jsUrl }}"></script>
  {% endfor %}
{% endif %}

{# Javascript definitions from Media #}
{% if js_def is not empty %}
<script type="text/javascript">
  {% for jsVar, jsValue in js_def %}
    var {{ jsVar }} = {{ jsValue|json_encode|raw }};
  {% endfor %}
</script>
{% endif %}

{% if display_header %}
  {{ renderhook('displayBackOfficeHeader', {}) }}
{% endif %}
