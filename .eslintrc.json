{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "impliedStrict": true,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "max-depth": [2, 3]
  }
}
