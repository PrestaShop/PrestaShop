# Apache 2.2
<IfModule !mod_authz_core.c>
    Order allow,deny
    Allow from all
    <FilesMatch "\.(twig)$">
      Order deny,allow
      Deny from all
    </FilesMatch>
</IfModule>

# Apache 2.4
<IfModule mod_authz_core.c>
    Require all granted
    <FilesMatch "\.(twig)$">
       Require all denied
    </FilesMatch>
</IfModule>
